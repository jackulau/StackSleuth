{
  "name": "stacksleuth",
  "version": "0.2.4",
  "description": "Open-source, real-time performance profiling tool for full-stack applications with advanced database integrations, framework support, and browser automation",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/fullstack-demo/api",
    "examples/fullstack-demo/web",
    "examples/svelte-demo",
    "examples/fastapi-demo",
    "examples/django-demo",
    "examples/laravel-demo"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "dev": "npm run dev --workspaces",
    "test": "npm run test --workspaces --if-present",
    "lint": "npm run lint --workspaces --if-present",
    "lint:fix": "npm run lint --workspaces --if-present -- --fix",
    "format": "prettier --write .",
    "clean": "npm run clean --workspaces",
    "sleuth": "node packages/cli/dist/index.js",
    "prepublish": "npm run build",
    "publish:all": "npm run build && npm publish --workspace=@stacksleuth/core && npm publish --workspace=@stacksleuth/backend-agent && npm publish --workspace=@stacksleuth/frontend-agent && npm publish --workspace=@stacksleuth/vue-agent && npm publish --workspace=@stacksleuth/db-agent && npm publish --workspace=@stacksleuth/mongodb-agent && npm publish --workspace=@stacksleuth/cli",
    "benchmark": "npm run benchmark --workspaces",
    "docs:build": "npm run docs:build --workspaces",
    "docs:dev": "npm run docs:dev --workspaces",
    "coverage": "npm run coverage --workspaces",
    "performance:test": "npm run performance:test --workspaces"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autocannon": "^7.15.0",
    "benchmark": "^2.1.4",
    "clinic": "^12.0.0",
    "cypress": "^13.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "playwright": "^1.52.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "performance",
    "profiling",
    "monitoring",
    "tracing",
    "fullstack",
    "developer-tools",
    "apm",
    "observability",
    "react",
    "svelte",
    "vue",
    "express",
    "fastapi",
    "django",
    "laravel",
    "nodejs",
    "postgresql",
    "mysql",
    "redis",
    "supabase",
    "browser-automation",
    "session-replay",
    "ci-cd"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Jack-GitHub12/StackSleuth.git"
  },
  "homepage": "https://github.com/Jack-GitHub12/StackSleuth#readme",
  "bugs": {
    "url": "https://github.com/Jack-GitHub12/StackSleuth/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "author": {
    "name": "Jack",
    "url": "https://github.com/Jack-GitHub12"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.50.0",
    "@types/ws": "^8.18.1",
    "axios": "^1.9.0",
    "cheerio": "^1.1.0",
    "ioredis": "^5.6.1",
    "mysql2": "^3.14.1",
    "puppeteer": "^24.10.0",
    "redis": "^5.5.6",
    "svelte": "^5.33.19",
    "ws": "^8.18.2"
  }
}
