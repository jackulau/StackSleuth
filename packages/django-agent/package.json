{
  "name": "@stacksleuth/django-agent",
  "version": "0.2.4",
  "description": "Advanced Django performance monitoring agent - Middleware tracking, database query optimization, template rendering analysis, session monitoring, and comprehensive view-level performance metrics.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "src/",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ci": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist",
    "benchmark": "node benchmarks/django-performance.js",
    "performance:test": "clinic doctor -- node benchmarks/memory-test.js"
  },
  "keywords": [
    "performance",
    "monitoring",
    "profiling",
    "stacksleuth",
    "observability",
    "analytics",
    "optimization",
    "instrumentation",
    "apm",
    "real-time",
    "django",
    "python",
    "orm",
    "middleware"
  ],
  "dependencies": {
    "@stacksleuth/core": "^0.2.1",
    "axios": "^1.6.0",
    "nodemailer": "7.0.12",
    "puppeteer": "24.34.0",
    "vite": "7.3.0",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "benchmark": "^2.1.4",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.16"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jackulau/StackSleuth.git",
